{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "corejs": "2.6.10",
        "targets": {
          "ie": 10
        }
      }
    ],
    "@babel/preset-typescript",
    [
      "@babel/preset-react"
    ]
  ],
  "plugins": [
      // 解决多个地方使用相同代码导致打包重复的问题
      ["@babel/plugin-transform-runtime"],
      ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ],
  "ignore": [
      "node_modules/**"
    ]
}
